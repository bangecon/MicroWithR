bc <- function(Q_H, Q_F, p_b) {
Qb_H = Q_H[1]
Qd_H = Q_H[2]
Qb_F = Q_F[1]
Qd_F = Q_F[2]
bc_H = p_b * Qb_H + Qd_H - (p_b * e_H[1] + e_H[2]) # Homer's budget constraint
bc_F = p_b * Qb_F + Qd_F - (p_b * e_F[1] + e_F[2]) # Flanders' budget constraint
rc = Qb_H + Qb_F  - (e_H + e_F) # resource constraint
c(bc_H, bc_F, rc)
}
